{
  "name": "aether/clock",
  "description": "The Aether clock package.",
  "license": "MIT",
  "type": "library",
  "support": {
    "issues": "https://github.com/sxbrsky/aether/issues",
    "source": "https://github.com/sxbrsky/aether"
  },
  "require": {
    "php": "^8.2",
    "psr/clock": "^1.0"
  },
  "provide": {
    "psr/clock-implementation": "*"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Aether\\Clock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aether\\Tests\\Clock\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
